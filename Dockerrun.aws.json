{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"client",
            "image":"16049921/multi-client",
            "hostName":"client",
            "essential":false,
            "memory":128
        },
        {
            "name":"server",
            "image":"16049921/multi-server",
            "hostName":"api",
            "essential":false,
            "memory":128
        },
        {
            "name":"worker",
            "image":"16049921/multi-worker",
            "hostName":"worker",
            "essential":false,
            "memory":128
        },
        {
            "name":"nginx",
            "image":"16049921/multi-nginx",
            "hostName":"nginx",
            "essential":true,
            "portsMapping":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client","server"],
            "memory":128
        }
    ]
}